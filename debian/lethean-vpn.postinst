#!/bin/sh -e

if [ "$1" = "configure" ]; then
    if ! getent group netdev | grep lthn > /dev/null 2>&1 ; then
        adduser --quiet lthn netdev
    fi
    /usr/lib/lthn/fixdevnet.sh
fi


# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
